Private Sub Form_Load()
Randomize
For i = 0 To Line1.UBound
X = Rnd * Form1.Width: Y = Rnd * Form1.Height
Line1(i).X1 = X
Line1(i).Y1 = Y
Line1(i).X2 = X
Line1(i).Y2 = Y
Line1(i).BorderColor = RGB(255, 255, 255)
Line1(i).BorderWidth = 3
Next i
Form1.BackColor = RGB(0, 0, 0)
Form1.Caption = "送你九十九颗星"
Timer1.Interval = 99

End Sub
Private Sub Timer1_Timer()
Randomize
For i = 0 To Line1.UBound
 X = Rnd * 61 - 30: Y = Rnd * 61 - 30  '每次移动的大小
   Rem 保证不出屏幕
 If Line1(i).X1 <= 0 Then X = Abs(X): If Line1(i).X1 >= Form1.Width Then X = -Abs(X)
 If Line1(i).Y1 <= 0 Then Y = Abs(Y): If Line1(i).Y2 >= Form1.Height Then Y = -Abs(Y)
 Line1(i).X1 = Line1(i).X1 + X: Line1(i).Y1 = Line1(i).Y1 + Y
 X2 = Line1(i).X1: Y2 = Line1(i).Y1
 Line1(i).X2 = X2: Line1(i).Y2 = Y2
Next i

End Sub
